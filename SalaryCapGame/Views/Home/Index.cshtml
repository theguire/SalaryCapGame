@{ 
    ViewData[ "Title" ] = "Game Administration";
}

<h3>@ViewData[ "Title" ]</h3>

<div class="mdl-grid">
    <div class="mdl-cell mdl-cell--6-col">
        <div class="demo-card-square mdl-card mdl-shadow--2dp">
            <div class="mdl-card__title mdl-card--expand" id="owners">
                <h2 class="mdl-card__title-text homeLabel"></h2>
            </div>
            <div class="mdl-card__actions mdl-card--border">
                <a class="mdl-button mdl-button--colored" asp-controller="Owner" asp-action="Index">
                    Owners and Franchises
                </a>
            </div>
        </div>
    </div>

    <div class="mdl-cell mdl-cell--6-col">
        <div class="demo-card-square mdl-card mdl-shadow--2dp">
            <div class="mdl-card__title mdl-card--expand" id="Leagues">
                <h2 class="mdl-card__title-text homeLabel"></h2>
            </div>

            <div class="mdl-card__actions mdl-card--border">
                <a class="mdl-button mdl-button--colored" asp-controller="Leagues" asp-action="Index">
                    Leagues
                </a>
            </div>
        </div>
    </div>
</div>