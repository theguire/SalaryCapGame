@model IEnumerable<SalaryCapData.Models.League>
@foreach ( var item in Model )
{
    <thead>
        <tr>
            <th>League</th>
            <th>Size</th>
            <th>Number of Franchises</th>
        </tr>
    </thead>
    <tr>
        <td><a asp-controller="League" asp-action="Detail" asp-route-id="@item.Id">@item.Name</a></td>
        <td>@item.MaxNumberFranchises</td>
    </tr>

}
